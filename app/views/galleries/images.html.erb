<!-- Add jQuery library -->
<script type="text/javascript" src="http://code.jquery.com/jquery-latest.min.js"></script>

<!-- Add mousewheel plugin (this is optional) -->
<script type="text/javascript" src="/fancybox/lib/jquery.mousewheel-3.0.6.pack.js"></script>

<!-- Add fancyBox -->
<link rel="stylesheet" href="/fancybox/     /jquery.fancybox.css?v=2.1.5" type="text/css" media="screen" />
<script type="text/javascript" src="/fancybox/source/jquery.fancybox.pack.js?v=2.1.5"></script>


<%= stylesheet_link_tag "jquery.fancybox.css?v=2.1.5" %>
<script type="text/javascript" src="http://code.jquery.com/jquery-latest.min.js"></script>
<%= javascript_include_tag "jquery.mousewheel-3.0.6.pack.js" %>
<%= javascript_include_tag "jquery.fancybox.pack.js?v=2.1.5" %>


<%= form_for(:image, :url => create_image_galleries_path, :html=>{:multipart => true}, :method => "post") do |f| %>
  <%= file_field :image, :image%>
  <%= hidden_field :image, :gallery_id, :value => @gallery.id %>
  <%= submit_tag "Save" %>
<% end %>
</br>
<% for image in @gallery.images %>
  <%= image_tag image.image_url(:thumb) if image.image?%>
<% end %>