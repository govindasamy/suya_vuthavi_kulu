<table id="table_style">
	<tr>
		<td>Location : </td>
		<td><%= collection_select :location_transaction, :location_id, @locations, :id, :name %></td>
	</tr>
	<tr>
		<td>Member : </td>
		<td><%= collection_select :location_transaction, :member_id, @members, :id, :name %></td>
	</tr>
	<tr>
		<td>Date: </td>
		<td><%= date_select :location_transaction, :date,:start_year => 1900, :end_year => Time.now.year, :size => "30" %></td>
	</tr>
	<tr>
		<td>Credit Amount : </td>
		<td><%= text_field :location_transaction, :credit_amount, :size => "30" %></td>
	</tr>
	<tr>
		<td>Debit Amount : </td>
		<td><%= text_field :location_transaction, :debit_amount, :size => "30" %></td>
	</tr>
	
</table>
