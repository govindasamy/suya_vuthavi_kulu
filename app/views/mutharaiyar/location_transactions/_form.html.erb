<table id="table_style">
	<tr>
		<td>Location : </td>
		<td><%= collection_select :location_transaction, :location_id, @locations, :id, :name, {:prompt => "--select--"} %></td>
	</tr>
	<tr>
		<td>Member : </td>
		<td><%= collection_select :location_transaction, :member_id, @members, :id, :name, {:prompt => "--select--"} %></td>
	</tr>
	<tr>
		<td>Date: </td>
		<td><%= text_field :location_transaction, :date, :size => "8", :class => "datepicker" %></td>
	</tr>
	<tr>
		<td>Credit Amount : </td>
		<td><%= text_field :location_transaction, :credit_amount, :size => "30" %></td>
	</tr>
	<tr>
		<td>Debit Amount : </td>
		<td><%= text_field :location_transaction, :debit_amount, :size => "30" %></td>
	</tr>
	
</table>
