<table><tr><td><H2>Memebr Details</H2></td></tr></table>
<p style='margin-left:273px'><%= link_to "<button>New Member</button>".html_safe, new_mutharaiyar_member_path %>	</p>
<table id="table_style">
	<tr>
		<th>Name</th>
		<th>Father Name</th>
		<th>Phone</th>
		<th>Date of Birth </th>
		<th>Profession </th>
		<!--th></th-->
	</tr>	
	<% for member in @members %>
	  <tr>
		<td><%= member.name %></td>
		<td><%= member.father_name %></td>
		<td><%= member.phone %></td>
		<td><%= member.date_of_birth %></td>
		<td><%= member.profession %></td>
		<td><%= link_to "Edit", edit_mutharaiyar_member_path(member) %></td>
		<td><%= link_to "Delete", mutharaiyar_member_path(member), :confirm => "Are you sure?", :method=>:delete %></td>
	  </tr>	
	<% end %>
</table>	
